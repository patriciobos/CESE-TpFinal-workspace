#include "http_cgi.h"
#include "httpd.h"

tCGI cgi_handler;


//Initialise cgi environment
int CGIinit( void) {
	cgi_handler.pcCGIName = "/actuators.cgi";
	cgi_handler.pfnCGIHandler = actuatorHandler;
	http_set_cgi_handlers(&cgi_handler, 1);
	return 0;
}
